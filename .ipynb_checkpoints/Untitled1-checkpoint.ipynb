    "# Problem 12\n",
    "from functools import reduce\n",
    "def factors(n):\n",
    "    return set(reduce(list.__add__,\n",
    "                ([i, n//i] for i in range(1, int(n**0.5) + 1) if n % i == 0)))\n",
    "n = 0\n",
    "a = 1\n",
    "i = 1\n",
    "while n <= 500:\n",
    "    i = i+1\n",
    "    a = a+i\n",
    "    n = len(factors(a))\n",
    "\n",
    "print(a)"
   ]
